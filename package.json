{
  "name": "marketing-engine-ai-gateway",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/ai-gateway.mjs",
    "start": "node server/ai-gateway.mjs",
    "gateway:dev": "nodemon server/analyticsGateway.mjs",
    "gateway:start": "node server/analyticsGateway.mjs",
    "lint": "eslint .",
    "web:dev": "vite",
    "web:build": "tsc -b && vite build",
    "web:preview": "vite preview",
    "analytics:dev": "vite --config vite.analytics.config.ts",
    "analytics:build": "tsc -b && vite build --config vite.analytics.config.ts",
    "analytics:build:vercel": "vite build --config vite.analytics.config.ts && cp dist/analytics.html dist/index.html",
    "test": "node --test tests",
    "verify": "npm run lint -- --max-warnings=0 && npm test",
    "monitor:health": "node scripts/monitor/health-check.mjs"
  },
