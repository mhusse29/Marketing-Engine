# Supabase Configuration (for main app with user authentication)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Server Configuration
VITE_API_URL=http://localhost:8787
VITE_ANALYTICS_GATEWAY_URL=http://localhost:8788

# Analytics Gateway Key (for standalone analytics authentication)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# Set BOTH on client AND server:
VITE_ANALYTICS_GATEWAY_KEY=your_secure_gateway_key_here
ANALYTICS_GATEWAY_KEY=your_secure_gateway_key_here

# Server-side only (do not expose to client)
SUPABASE_SERVICE_KEY=your_service_role_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
